pipeline {
   agent any
   stages {
       stage('Build Code From DEV') {
           steps {
               sh """
               echo "Building Artifact"
               """
           }
       }
    stages {
       stage('Build CodeFrom Dev') {
           steps {
               sh """
               echo "Building Artifact"
               """
           }
       }
      stage('Deploy Code From Dev') {
          steps {
               sh """
               echo "Deploying Code1"
               """
          }
      }
   }
}

}
